        <rdfs:range rdf:resource="http://data.opendiscoveryspace.eu/lom_ontology_ods.owl#DateTime"/>
        <rdfs:comment xml:lang="en">Date that this annotation was created. IEEE LOM element 8.2.</rdfs:comment>
    </owl:ObjectProperty>
    


